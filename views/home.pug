				.form-group
					label(for='id') Usuario
					input#id.form-control(type='text', placeholder='Usuario')
				.form-group
					label(for='pass') Password
					input#pass.form-control(type='password', placeholder='Password')
				button.btn.btn-primary(type='submit', onclick="login()") Login

			#etiqueta(onclick='accion()')
